{
   "body": "<!DOCTYPE html>\n\
<html lang=\"ru\">\n\
   <head>\n\
      <meta http-equiv=\"Content-Type\" content=\"text/html; charset=UTF-8\">\n\
      <title>{{title}}</title>\n\
      {{script}}\n\
      {{style}}\n\
   </head>\n\
   <body>{$page$}</body>\n\
</html>",
   "title":{
      "format":"input",
      "type":"text",
      "name":"title"
   },
   "script":{
      "format":"control",
      "system":true,
      "control":"scriptList",
      "data":{
         "scripts":[
            "web-morpher/ui/ext/jquery.js",
            "web-morpher/ui/core.js",
            "web-morpher/ulib/parser.js"
         ]
      }
   },
   "style":{
      "format":"control",
      "system":true,
      "control":"styleList",
      "data":{
         "styles":[
            "web-morpher/ui/ext/bootstrap/css/bootstrap.min.css"
         ]
      }
   }
}

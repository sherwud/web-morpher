{
   "logFilesRoot":"./",
   "stackTraceLimit":15,
   "defaultLogCode":"100",
   "glueDisplayMessages":false,
   "glueFileMessages":true,
   "logCode":{
      "0":{
         "type":"DONE"
      },
      "1":{
         "type":"ERROR"
      },
      "100":{
         "type":"UNDEFINED ERROR",
         "inFile":false,
         "path":"./log/error.log",
         "DateInPath":true,
         "sync":true
      },
      "2":{
         "type":"INFO"
      },
      "3":{
         "hide": true,
         "type":"SYSTEM",
         "inFile":true,
         "path":"./log/system.log",
         "DateInPath":true
      },
      "404":{
         "type":"NOT FOUND",
         "inFile":true,
         "path":"./log/404error.log",
         "DateInPath":true
      }
   }
}
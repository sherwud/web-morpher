{
   "config":{
      "cached":false,
      "template":{
         "name":"main",
         "standard":false,
         "input":{
            "title":"Info - Test"
         }
      },
      "element":"page",
      "standard":false,
      "params":{
         "class":"infoPage"
      }
   },
   "body":"{{HEADER}}\n\
   <div class=\"aside-left\">\n\
   <div class=\"aside-menu shadowbox\">{{MAINMENU}}{{ADDBUTTON}}</div>\n\
   {{UPLOAD}}</div>\n\
   <div id=\"wm-content\" class=\"shadowbox\">{{NODEJSINFO}}</div>\n\
   {{EDITBUTTON}}\n\
   {{FOOTER}}\n\
   {{loading}}{{editing}}{{additemform}}{{edititemform}}",
   "params":{
      "HEADER":"<h3 class=\"wm-header shadowbox\">\n\
        <span>Node.js</span>\n\
        <i>Тут должна быть ваша цитата</i></h3>",
      "MAINMENU":{
         "ruName":"Главное меню",
         "description":"Основное меню сайта",
         "format":"elemetn",
         "system":false,
         "elemetn":"mainmenu",
         "data":{
            "class":"my-mainmenu"
         }
      },
      "ADDBUTTON":"<div class=\"menubutton add\"><div></div>add</div>",
      "EDITBUTTON":"<div class=\"menubutton editPage hide\"><div></div>edit</div>",
      "FOOTER":"<div class=\"wm-footer\">\n\
         <span>Спасибо: </span>\n\
         <a target=\"blank\" href=\"http://nodejs.org\">Node.js</a>\n\
         <span> | </span>\n\
         <a target=\"blank\" href=\"https://github.com\">GitHub</a>\n\
         <span> | </span>\n\
         <a target=\"blank\" href=\"http://jquery.com\">jQuery</a>\n\
         <span> | </span>\n\
         <a target=\"blank\" href=\"http://twitter.github.com/bootstrap/\">Bootstrap</a>\n\
         <span>Автор: </span>\n\
         <a href=\"https://github.com/IDrakon\">Dr@KoN</a>\n\
         <span>CMS: </span>\n\
         <a target=\"blank\" href=\"https://github.com/IDrakon/web-morpher\">Web Morpher</a>\n\
         </div>",
      "NODEJSINFO":{
         "ruName":"Информация о Node.JS",
         "description":"Элемент с информацией о Node.JS",
         "format":"elemetn",
         "system":false,
         "elemetn":"nodejsinfo"
      },
      "UPLOAD":"<form class=\"wm-upload\" enctype=\"multipart/form-data\" method=\"post\" action=\"/wm\">\n\
         Загрузить картинку\n\
         <input type=\"text\" class=\"hide\" name=\"call\" value=\"info.upload\">\n\
         <input size=\"17\" type=\"file\" accept=\"image/*\" name=\"image\">\n\
         <input type=\"submit\" value=\"Отправить\">\n\
         </form>",
      "loading":"<div class=\"wm-loading wm-blocked\"><div>Загрузка...</div></div>",
      "editing":"<div class=\"wm-editing wm-blocked hide\"></div>",
      "additemform":"<div class=\"wm-additemform wm-itemform hide shadowbox\">\n\
      <div class=\"wm-close menubutton\"><div></div>X</div>\n\
      <div class=\"wm-add menubutton\"><div></div>add</div>\n\
      <div class=\"wm-art-header\">Добавить статью</div>\n\
      <span>Название</span>\n\
      <input type=\"text\" name=\"name\" style=\"margin-bottom: 4px; width: 200px;\"></input>\n\
      <span>Позиция</span>\n\
      <input type=\"text\" name=\"sort\" style=\"margin-bottom: 4px; width: 40px;\"></input>\n\
      <span style=\"display: block;\">HTML код статьи</span>\n\
      <textarea name=\"html\" style=\"resize: none; width: calc(100% - 6px); height: calc(100% - 81px);\"></textarea>\n\
      </div>",
      "edititemform":"<div class=\"wm-edititemform wm-itemform hide shadowbox\">\n\
      <div class=\"wm-close menubutton\"><div></div>X</div>\n\
      <div class=\"wm-save menubutton\"><div></div>save</div>\n\
      <div class=\"wm-del menubutton\"><div></div>del</div>\n\
      <div class=\"wm-art-header\">Редактировать статью</div>\n\
      <span>Название</span>\n\
      <input type=\"text\" name=\"name\" style=\"margin-bottom: 4px; width: 200px;\"></input>\n\
      <span>Позиция</span>\n\
      <input type=\"text\" name=\"sort\" style=\"margin-bottom: 4px; width: 40px;\"></input>\n\
      <span style=\"display: block;\">HTML код статьи</span>\n\
      <textarea name=\"html\" style=\"resize: none; width: calc(100% - 6px); height: calc(100% - 81px);\"></textarea>\n\
      </div>"
   }
}